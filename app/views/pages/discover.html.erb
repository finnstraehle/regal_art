<div class="container-m">
  <h1>Discover</h1>
  <%= link_to "Log Out", destroy_user_session_path, data: {turbo_method: :delete} %>
  <% @artists.each do |artist| %>
    <div class="d-flex">
      <%= cl_image_tag artist.avatar.key, class: "avatar-square" %>
      <%= artist.first_name %>
    </div>
  <% end %>
</div>
